{
    "name": "vscode-ember-snow",
    "displayName": "Ember Grammar and Theme",
    "description": "Colorize Ember.js syntax and Handlebars templates.",
    "version": "0.0.1",
    "publisher": "gtb104",
    "icon": "images/snow.png",
    "engines": {
        "vscode": "^1.0.0"
    },
    "extensionDependencies": [
        "handlebars",
        "dzannotti.vscode-babel-coloring"
    ],
    "contributes": {
        "grammars": [{
            "label": "Handlebars (Ember)",
            "language": "handlebars",
            "scopeName": "text.html.handlebars",
            "path": "./syntaxes/handlebars.tmLanguage"
        }],
        "themes": [
            {
                "label": "Ember Snow",
                "uiTheme": "vs-dark",
                "path": "./themes/ember.tmTheme"
            }
        ]
    }
}
